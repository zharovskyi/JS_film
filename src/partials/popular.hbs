{{#each this}}
<li class="movie">
  <img class="movie-logo" src="https://image.tmdb.org/t/p/w500{{backdrop_path}}" alt="movie-title">
  <p class="movie-title">{{title}}{{release_date}}</p>
  <svg class="svg-bell">
    <use href="../img/sprite.svg#bell"></use>
  </svg>
  <svg class="svg-star">
    <use href="../img/sprite.svg#star-full"></use>
  </svg>
</li>
{{/each}}
